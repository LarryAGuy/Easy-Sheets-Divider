{
  "name": "easy-sheets-divider",
  "version": "1.0.0",
  "description": "A powerful, privacy-first web application for splitting large Excel, CSV, OpenOffice, and Google Sheets files into smaller, manageable spreadsheets",
  "main": "index.html",
  "scripts": {
    "start": "http-server -p 8000 -c-1",
    "dev": "http-server -p 8000 -c-1 -o",
    "build": "echo 'No build process needed - static files only'",
    "test": "echo 'Manual testing required - see CONTRIBUTING.md'",
    "lint": "echo 'Linting not configured - contributions welcome'",
    "serve": "python -m http.server 8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarryAGuy/Easy-Sheets-Divider.git"
  },
  "keywords": [
    "excel",
